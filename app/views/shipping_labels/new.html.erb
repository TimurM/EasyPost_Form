<nav>
  <h1>Dot&Bo</h1>
</nav>

<form id="myform" class="mysign-in" action="<%= shipping_labels_url %>" method="post">
  <input
  type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">

  <div class="form-container">
    <p>To Address</p>
    <div class="inputs">
        <input type="text" name="to_address[name]" class="form-control" placeholder="name" required>
    </div>

    <div class="inputs">
        <input type="text" name="to_address[company]" class="form-control" placeholder="company" required>
    </div>
    <div class="inputs">
        <input type="text" name="to_address[street1]" class="form-control" placeholder="stree1" required>
    </div>
    <div class="inputs">
        <input type="text" name="to_address[city]" class="form-control" placeholder="city" required>
    </div>

    <div class="inputs">
        <input type="text" name="to_address[state]" class="form-control" placeholder="state" required>
    </div>
    <div class="inputs">
        <input type="text" name="to_address[zip]" class="form-control" placeholder="zip" required>
    </div>
  </div>

  <div class="form-container">
    <p>From Address</p>
    <div class="inputs">
        <input type="text" name="from_address[company]" class="form-control" placeholder="company" required>
    </div>

    <div class="inputs">
        <input type="text" name="from_address[stree1]" class="form-control" placeholder="stree1" required>
    </div>
    <div class="inputs">
        <input type="text" name="from_address[street2]" class="form-control" placeholder="stree2">
    </div>
    <div class="inputs">
        <input type="text" name="from_address[city]" class="form-control" placeholder="city" required>
    </div>

    <div class="inputs">
        <input type="text" name="from_address[state]" class="form-control" placeholder="state" required>
    </div>
    <div class="inputs">
        <input type="number" name="from_address[zip]" class="form-control" placeholder="zip" required>
    </div>
    <div class="inputs">
        <input type="tel" name="from_address[phone]" class="form-control" placeholder="phone">
    </div>
  </div>

  <div class="form-container">
    <p>Parcel Dimentions</p>
    <div class="inputs">
        <input type="number" name="parcel[length]" class="form-control" placeholder="length" required>
    </div>

    <div class="inputs">
        <input type="number" name="parcel[width]" class="form-control" placeholder="width" required>
    </div>
    <div class="inputs">
        <input type="number" name="parcel[height]" class="form-control" placeholder="height" required>
    </div>
    <div class="inputs">
        <input type="number" name="parcel[weight]" class="form-control" placeholder="weight" required>
    </div>
  </div>

  <div class="inputs">
      <button type="submit" class="button">Print Label</button>
  </div>
</form>
</div>
